//a series of 3n+2 not divisible by 4
#include<iostream>
using namespace std;

int main(){
    int x;
    cin>>x;
    int n=1;
    int count=0;
    while(count<=x){
        int i=3*n+2;
        if(i%4!=0){
            cout<<i<<' ';
            count++;
        }
        n++;
    }
    cout<<count<<endl;
}
