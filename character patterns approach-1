//character patterns approach-1
#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=1;
        char val=65;
        while(j<=n){
            cout<<val;
            val++;
            j++;
        }
        cout<<endl;
        i++;
    }
}
