#include <iostream>
using namespace std;
int fact(int n){
    int i=1;
    int ans=1;
    if(n==0){
        return ans;
    }
    else{
        while(i<=n){
        ans*=i;
        i++;
        }
    return ans;
    }
}



int main(){
    int n;
    cin>>n;
    int ans=fact(n);
    cout<<ans<<endl;
}
